#include <iostream>
using namespace std;

int main() {
    for (int i = 100; i <= 300; i++) { // перебираем числа от 100 до 300
        int sum = 0;
        for (int j = 1; j <= i /2 ; j++) { // перебираем делители числа
            if (i % j ==0) {
                sum +=j;
            }
        }
        if(sum ==50) { // если сумма делителей равна нужному значению, выводим число на экран
            cout <<i <<" ";
        }
    }

    return 0;
}
