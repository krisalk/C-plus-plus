#include <iostream>
#include <algorithm> // для функции sort
using namespace std;

int main() {
    int arr[3][4] = {{3, 2, 1, 4}, {8, 6, 7, 5}, {11, 10, 12, 9}};

    // сортировка последней строки
    sort(arr[2], arr[2] + 4);

    // вывод отсортированного массива
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 4; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
