#include <iostream>
using namespace std;

int main() {
    for (int i = 30; i >= 26; i--) { // внешний цикл от 30 до 26
        for (int j = i; j <= 30; j++) { // внутренний цикл от текущего числа до 30
            cout << j << " "; // выводим число и пробел после него
        }
        cout << endl; // переходим на новую строку после каждой строки таблицы
    }
    return 0;
}
