#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x;
    const int N = 10; // граница ряда
    cout << "Введите значение x (-1 < x < 1): ";
    cin >> x;
    double sum = 0.0;
    double term = x;
    for (int n = 1; n <= N; n++) {
        sum += term;
        term *= -x * n / (n + 1);
    }
    cout << "ln(1 + " << x << ") = " << sum << endl;
    return 0;
}

// самостоятельная работа 4 базовый задание 8.27
#include <iostream>
using namespace std;

int main() {
    for (int i = 1; i <= 300; i++) {
        int count = 0;
        for (int j = 1; j <= i; j++) {
            if (i % j == 0) {
                count++;
            }
        }
        if (count == 5) {
            cout << i << " ";
        }
    }
    cout << endl;
    return 0;
}
