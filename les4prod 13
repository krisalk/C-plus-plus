#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n;
    double xi, x = 0, m2 = 0;
    double arr[100];

    cout << "Введите количество элементов массива: ";
    cin >> n;

    cout << "Введите элементы массива: ";

    for (int i = 0; i < n; i++) {
        cin >> arr[i];
        x += arr[i];
    }

    x /= n;

    for (int i = 0; i < n; i++) {
        m2 += pow(arr[i] - x, 2);
    }

    m2 /= n;

    cout << "Выборочная дисперсия: " << m2 << endl;

    return 0;
}
